/* pages/address/address.wxss */

page {
  line-height: 1.6;
  font-family: -apple-system-font, Helvetica Neue, sans-serif;
}

.address_container {
  margin: 0;
  padding: 0;
  min-height: 100%;
  background-color: #f5f5f5;
}

.address_header {
  font-size: 17PX;
  width: 100%;
  text-align: center;
  font-weight: normal;
  background: white;
  padding: 3% 0;
  margin-bottom: 1px;
}

.select_address {
  background: white;
  padding: 2% 3%;
  display: flex;
  color: #000;
  word-wrap: break-word;
  font-size: 0.875rem;
}

.selectde_city {
  position: relative;
  width: 20%;
  background-color: #f5f5f5;
  padding: 2%;
  border-top-color: 0.2rem;
  border-bottom-left-radius: 0.2rem;
}

.selectde_city::after {
  content: '';
  width: 0.625rem;
  height: 0.625rem;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0.8rem;
  margin-left: 0.4rem;
}

.selected_input {
  background-color: #f5f5f5;
  position: relative;
  padding: 2%;
  padding-left: 1rem;
  width: 80%;
  border-top-right-radius: 0.2rem;
  border-bottom-right-radius: 0.2rem;
}

.selected_input::before {
  content: '';
  width: 0.875rem;
  height: 0.875rem;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0.75rem;
  margin-left: -1rem;
}

.notify {
  font-size: 0.75rem;
  color: #f4a22d;
  padding-left: 0.9375rem;
  height: 2.25rem;
  line-height: 2.25rem;
}

.address_location {
  width: 100%;
  padding: 2% 3%;
  background: white;
  font-size: 0.875rem;

}
.address_location_container{
  position: relative;
  padding-left: 1.5rem;
}
.address_location_container::before {
  content: '';
  width: 1.375rem;
  height: 1.375rem;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  margin-left: -1.5rem;
}

.address_location_container::after {
  content: '';
  width: 1.375rem;
  height: 1.375rem;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0.9375rem;
}

.local_address {
  color: #969696;
  display: inline-block;
  margin-left: 0.5rem;
}
