/* pages/searchAddress/searchAddress.wxss */
.search_address_container{
  background-color: #f5f5f5;
  min-height: 100%;
  
}
.search_container{
  background: white;
   padding: 2% 3%;
   position: relative;
}
.selected_input {
  background-color: #f5f5f5;
  position: relative;
 border-radius: 0.3rem;
 
  padding: 0.2rem 0 0.2rem 1.2rem;
  
}

.selected_input::before {
  content: '';
  width: 1rem;
  height: 1rem;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
   margin: auto;
   margin-left: -1rem;
}

.address_title{
      margin: 0;
    font-size: 0.75rem;
    font-weight: 700;
}
.address_address{
      padding-top: 0.1875rem;
    font-size: 0.7125rem;
}
.address_list{
      color: #4C4440;
    border-bottom: 0.0625rem solid #f5f5f5;
    padding: 0.3125rem 0;
}
.clear{
      background: url(data:image/png;base64,) no-repeat;
    color: transparent;
    background-size: 100% 100%;
    display: inline-block;
    width: 1.25rem;
    height: 1.25rem;
    position: absolute;
    right: 1.25rem;
    top: 0.625rem;
    z-index: 200;
}